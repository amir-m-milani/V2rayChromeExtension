{
    "manifest_version": 3,
    "name": "VPN Extension",
    "version": "1.0",
    "description": "A simple VPN extension using V2ray",
    "permissions": [
        "storage",
        "background",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png"
    }
}